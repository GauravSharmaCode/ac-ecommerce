# .env example for Django on Render
DJANGO_SECRET_KEY=your-production-secret-key
DJANGO_DEBUG=False
DJANGO_ALLOWED_HOSTS=your-app.onrender.com
DJANGO_CORS_ALLOWED_ORIGINS=https://your-frontend-domain.com

# Cloudinary (if/when you use it)
CLOUDINARY_STORAGE__CLOUD_NAME=your_cloud_name
CLOUDINARY_STORAGE__API_KEY=your_api_key
CLOUDINARY_STORAGE__API_SECRET=your_api_secret
